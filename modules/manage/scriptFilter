#!/usr/bin/env bash
# @import array/includes

scriptFilter () {
    local script
    local path

    script="${SCRIPTNAMES[$2]}"
    path="${SCRIPTPATHS[$2]}"

    if [[ "${script:0:1}" == "_" ]]
    then
        if includes MANAGEPUBLIC "${script:8}"
        then
            "$3" "${path}"
        fi
    elif [[ "${script}" != "completion" ]]
    then
        "$3" "${path}"
    fi
}
